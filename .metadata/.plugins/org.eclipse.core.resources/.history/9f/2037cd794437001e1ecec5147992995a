package di_p;

public class SmartPhone {
	String name;
	Battery bat;
	Mscreen msc;
	Mcamera mca;
	
	public SmartPhone() {
		System.out.println("SmartPhone 기본생성자");
	}
	
	
	public void setName(String name) {
		this.name = name;
	}
	public void setBat(Battery bat) {
		this.bat = bat;
	}
	public void setMsc(Mscreen msc) {
		this.msc = msc;
	}
	public void setMca(Mcamera mca) {
		this.mca = mca;
	}
	
	
}

class Battery{
	String name;
	int Ah;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAh() {
		return Ah;
	}
	public void setAh(int ah) {
		Ah = ah;
	}
	
	
}

class Mscreen{
	String name;
	int size;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
	
}

class Mcamera{
	String name;
	int pixel;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPixel() {
		return pixel;
	}
	public void setPixel(int pixel) {
		this.pixel = pixel;
	}
	
}


