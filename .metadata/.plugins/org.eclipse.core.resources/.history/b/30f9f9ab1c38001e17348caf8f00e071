package anno_p;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

public class Coffee {

	@Autowired(required = true)
	String name;
	int price;
	
	@Autowired
	@Qualifier("")
	Beans beans;
	
	@Autowired(required = false)
	Ingredients ingre;

	@Override
	public String toString() {
		return "Coffee [name=" + name + ", beans=" + beans + ", ingre=" + ingre + ", price=" + price + "]";
	}
	
}

class Beans{
	String region, name;
}

class Ingredients{
	String type, name;
}